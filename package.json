{
  "name": "shadowrealm-api",
  "version": "0.6.0",
  "description": "ShadowRealm polyfill for browser",
  "author": {
    "name": "Ambit Tsai",
    "email": "ambit_tsai@qq.com"
  },
  "license": "Apache-2.0",
  "main": "browser/index.umd.js",
  "module": "browser/index.mjs",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "test262-harness --hostType=chrome --hostPath='/Applications/Google Chrome.app/Contents/MacOS/Google Chrome' --transformer=test/transformer.js --prelude=dist/browser/polyfill.umd.js --prelude=test/browser-helpers.js --test262Dir=test 'test/common/**/*.js'",
    "test:docs": "vite -c docs-src/vite.config.ts",
    "build:docs": "vite build -c docs-src/vite.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ambit-tsai/shadowrealm-api.git"
  },
  "homepage": "https://github.com/ambit-tsai/shadowrealm-api#readme",
  "bugs": {
    "url": "https://github.com/ambit-tsai/shadowrealm-api/issues"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "~8.3.0",
    "jest": "~27.4.3",
    "jest-puppeteer": "~6.0.2",
    "puppeteer-core": "~12.0.1",
    "rollup": "~2.59.0",
    "rollup-plugin-terser": "~7.0.2",
    "test262-harness": "~9.0.0",
    "tslib": "~2.3.1",
    "typescript": "~4.5.2",
    "vite": "^2.7.2"
  },
  "keywords": [
    "shadowrealm",
    "shadowrealms",
    "realm",
    "realms",
    "sandbox",
    "api",
    "polyfill",
    "ponyfill",
    "shim"
  ]
}
